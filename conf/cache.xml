<?xml version="1.0" encoding="UTF-8" ?>
<config>
    <CacheModule>
        <default_handler>store</default_handler>

        <handlers>
            <store>com.voidsearch.voidbase.apps.cache.module.MessagePersistenceStore</store>
        </handlers>
    </CacheModule>

    <MessagePersistenceStore>
        <storage>/data/storage/messages</storage>

        <operations>
            <GET lock="none"/>
            <PUT lock="atomic"/>
            <DELETE lock="atomic"/>
            <FLUSH lock="global"/>
        </operations>
    </MessagePersistenceStore>
</config>